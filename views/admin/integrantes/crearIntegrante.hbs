<body>
    <div class="content">
        <div class="container">
            <h1>Crear Nuevo Integrante</h1>
                <form action="/admin/entidad1/create" method="post">
                    <label for="nombre">Nombre:</label>
                    <input type="text" id="nombre" name="nombre" required><br>

                    <label for="apellido">Apellido:</label>
                    <input type="text" id="apellido" name="apellido" required><br>

                    <label for="matricula">Matr√≠cula:</label>
                    <input type="text" id="matricula" name="matricula" required><br>

                    <label for="activo">Activo:</label>
                    <input type="checkbox" id="activo" name="activo" value="1"><br>

                    <label for="orden">Orden:</label>
                    <input type="number" id="orden" name="orden" required><br>

                    <button type="submit" class="button-40">Enviar</button>
                </form>
